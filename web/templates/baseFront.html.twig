  <script>
      function greeting() {
        let searchlinput = document.getElementById("searchinput")
        const searchValue = searchlinput.value;
        console.log('searchValue',searchValue);
        $.ajax({
            type: "POST",
            url: "{{ path('demo_create')}}",
            data:{ search: searchValue, path:'ajax' },
            success: function (msg) {
                const content = document.querySelector("#commandeSection");
               content.innerHTML = msg.content;
               searchValue.value = searchValue
                           }
        });
    }

        $(document).ready(function(){
            $("#searchinput").keyup(greeting);
        });
    </script>


